    "#총합(시그마) 는 sum()을 통해 구현가능하다. \n",
    "\n",
    "import numpy as np\n",
    "\n",
    "a = np.array([1, 3, 2, 5, 4])\n",
    "sum_result = np.sum(a)\n",
    "multiple_result = np.prod(a)\n",
    "\n",
    "print(sum_result)\n",
    "print(multiple_result)\n"
   ]
